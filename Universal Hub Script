		TitleC.Position = UDim2.new(0, 15, 0, 10);
		TitleC.TextColor3 = Color3.new(1, 1, 1);
		TitleC.Font = Enum.Font.GothamBold;
		TitleC.TextSize = 22;
		TitleC.TextXAlignment = "Left";
		TitleC.BackgroundTransparency = 1;
		TitleC.ZIndex = 5001;
		local Msg = Instance.new("TextLabel", ConfirmFrame);
		Msg.Text = "VocÃª deseja realmente executar esse script?\n(" .. scriptName .. ")";
		Msg.Size = UDim2.new(1, -30, 0, 80);
		Msg.Position = UDim2.new(0, 15, 0, 45);
