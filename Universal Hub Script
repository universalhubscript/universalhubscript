	task.spawn(function()
		local startTime = tick();
		while (tick() - startTime) < 3 do
			local FlatIdent_1BAD7 = 0;
			local pulse;
			while true do
				if (FlatIdent_1BAD7 == 0) then
					pulse = 1 + (0.06 * math.sin(tick() * 4));
					IntroFrame.Size = UDim2.new(0, 350 * pulse, 0, 80 * pulse);
					FlatIdent_1BAD7 = 1;
				end
				if (FlatIdent_1BAD7 == 1) then
					IntroFrame.Position = UDim2.new(0.5, -(350 * pulse) / 2, 0.5, -(80 * pulse) / 2);
					task.wait();
					break;
				end
			end
		end
		TweenService:Create(IntroFrame, TweenInfo.new(0.6), {BackgroundTransparency=1}):Play();
		TweenService:Create(IntroStroke, TweenInfo.new(0.6), {Transparency=1}):Play();
		TweenService:Create(IntroText, TweenInfo.new(0.6), {TextTransparency=1}):Play();
		task.wait(0.6);
		IntroFrame:Destroy();
		FloatBtn.Visible = true;
		MainFrame.Size = UDim2.new(0, 0, 0, 0);
		MainFrame.Visible = true;
		MainFrame:TweenSize(UDim2.new(0, 620, 0, 380), "Out", "Back", 0.6, true);
		Notify("HUB ATIVO", "Bem-vindo ao Script Universal do Kira!");
		task.delay(1, ShowDiscordInvite);
	end);
	print("Script Universal Hub Iniciado!");
end
