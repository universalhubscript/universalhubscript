	local function Notify(title, msg)
		local FlatIdent_31A5A = 0;
		local NotifFrame;
		local ns;
		local t;
		local m;
		while true do
			if (FlatIdent_31A5A == 7) then
				m.BackgroundTransparency = 1;
				m.TextColor3 = Color3.fromRGB(200, 200, 200);
				m.Font = Enum.Font.Gotham;
				FlatIdent_31A5A = 8;
			end
			if (3 == FlatIdent_31A5A) then
