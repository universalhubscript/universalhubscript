	Instance.new("UICorner", FavTabBtn);
	FavTabBtn.MouseButton1Click:Connect(function()
		local FlatIdent_5F1CB = 0;
		local StatusLabel;
		local allFavs;
		local favListTbl;
		while true do
			if (FlatIdent_5F1CB == 2) then
				StatusLabel.Text = "Seus Scripts Favoritos";
				StatusLabel.TextColor3 = Color3.fromRGB(255, 215, 0);
				StatusLabel.Font = Enum.Font.GothamBold;
				FlatIdent_5F1CB = 3;
			end
			if (FlatIdent_5F1CB == 4) then
				favListTbl = {};
				for gName, scripts in pairs(allFavs) do
					for _, gData in pairs(Database) do
						if (gData.Name == gName) then
							for _, sName in pairs(scripts) do
								local func = gData.Key[sName] or gData.NoKey[sName];
								if func then
									favListTbl[sName] = {func=func,originalGameName=gName};
								end
							end
						end
					end
				end
				LoadScripts(favListTbl, {Name="Favoritos"});
				FlatIdent_5F1CB = 5;
			end
			if (FlatIdent_5F1CB == 0) then
				for _, v in pairs(ScriptList:GetChildren()) do
					if (v:IsA("Frame") or v:IsA("TextLabel") or v:IsA("TextButton") or v:IsA("UIPadding")) then
						v:Destroy();
					end
				end
				for _, v in pairs(TabArea:GetChildren()) do
					if (v:IsA("TextButton") or v:IsA("TextLabel")) then
						v:Destroy();
					end
				end
				StatusLabel = Instance.new("TextLabel", TabArea);
				FlatIdent_5F1CB = 1;
			end
			if (1 == FlatIdent_5F1CB) then
				StatusLabel.Size = UDim2.new(1, -10, 0, 30);
				StatusLabel.Position = UDim2.new(0, 5, 0, 5);
				StatusLabel.BackgroundTransparency = 1;
				FlatIdent_5F1CB = 2;
			end
			if (FlatIdent_5F1CB == 5) then
				Notify("LISTA", "Carregando seus favoritos...");
				break;
			end
			if (FlatIdent_5F1CB == 3) then
				StatusLabel.TextSize = 12;
				StatusLabel.TextWrapped = true;
				allFavs = GetFavorites();
				FlatIdent_5F1CB = 4;
			end
		end
	end);
	for i = 1, #Database do
		local FlatIdent_494F6 = 0;
