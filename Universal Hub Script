local TweenService = game:GetService("TweenService");
local Players = game:GetService("Players");
local HttpService = game:GetService("HttpService");
local LocalPlayer = Players.LocalPlayer;
local CorrectKey = "2026MUNDONOVO";
local function IniciarHubOriginal()
	local TweenService = game:GetService("TweenService");
	local Players = game:GetService("Players");
	local HttpService = game:GetService("HttpService");
	local LocalPlayer = Players.LocalPlayer;
	local FavFile = "SFZ_Favorites.json";
	if not isfile(FavFile) then
		writefile(FavFile, HttpService:JSONEncode({}));
	end
	local function GetFavorites()
		local success, content = pcall(function()
			return readfile(FavFile);
		end);
		if success then
			return HttpService:JSONDecode(content);
		end
		return {};
	end
	local function ToggleFavorite(scriptName, gameName)
		local FlatIdent_2584C = 0;
		local favs;
		local index;
		while true do
			if (FlatIdent_2584C == 0) then
				favs = GetFavorites();
				if not favs[gameName] then
					favs[gameName] = {};
				end
				FlatIdent_2584C = 1;
			end
			if (FlatIdent_2584C == 1) then
				index = table.find(favs[gameName], scriptName);
				if index then
					local FlatIdent_6D4CB = 0;
					while true do
						if (FlatIdent_6D4CB == 1) then
							writefile(FavFile, HttpService:JSONEncode(favs));
							return false;
						end
						if (FlatIdent_6D4CB == 0) then
							table.remove(favs[gameName], index);
							if (#favs[gameName] == 0) then
								favs[gameName] = nil;
							end
							FlatIdent_6D4CB = 1;
						end
					end
				else
					local FlatIdent_12703 = 0;
					while true do
						if (FlatIdent_12703 == 0) then
							table.insert(favs[gameName], scriptName);
							writefile(FavFile, HttpService:JSONEncode(favs));
							FlatIdent_12703 = 1;
						end
						if (FlatIdent_12703 == 1) then
							return true;
						end
					end
				end
				break;
			end
		end
	end
