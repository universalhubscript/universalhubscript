	local function LoadScripts(tbl, gameData)
		for _, v in pairs(ScriptList:GetChildren()) do
			if (v:IsA("Frame") or v:IsA("TextLabel") or v:IsA("TextButton") or v:IsA("UIPadding")) then
				v:Destroy();
			end
		end
		local count = 0;
		if tbl then
			for _ in pairs(tbl) do
				count = count + 1;
			end
		end
		if (count == 0) then
			local FlatIdent_E0D0 = 0;
			local Padding;
			local EmptyMsg;
			while true do
				if (FlatIdent_E0D0 == 0) then
					Padding = Instance.new("UIPadding", ScriptList);
					Padding.PaddingTop = UDim.new(0, 5);
					EmptyMsg = Instance.new("TextLabel", ScriptList);
					FlatIdent_E0D0 = 1;
				end
				if (FlatIdent_E0D0 == 2) then
					EmptyMsg.Text = "ðŸš« VAZIO...";
					EmptyMsg.TextColor3 = Color3.new(1, 1, 1);
					EmptyMsg.Font = Enum.Font.GothamBold;
					FlatIdent_E0D0 = 3;
				end
				if (FlatIdent_E0D0 == 1) then
					EmptyMsg.AutoLocalize = false;
					EmptyMsg.Size = UDim2.new(1, -10, 0, 30);
					EmptyMsg.BackgroundTransparency = 1;
					FlatIdent_E0D0 = 2;
				end
				if (FlatIdent_E0D0 == 3) then
					EmptyMsg.TextSize = 14;
					EmptyMsg.TextTransparency = 0.4;
					EmptyMsg.TextXAlignment = Enum.TextXAlignment.Center;
					break;
				end
			end
		else
			for name, data in pairs(tbl) do
				local FlatIdent_324DE = 0;
				local executeFunc;
				local originGame;
				local ItemFrame;
				local b;
				local favBtn;
				local UpdateStar;
				while true do
					if (FlatIdent_324DE == 2) then
						b.AutoLocalize = false;
						b.Size = UDim2.new(0.82, 0, 1, 0);
						b.BackgroundColor3 = Color3.fromRGB(110, 0, 190);
						FlatIdent_324DE = 3;
					end
					if (FlatIdent_324DE == 5) then
						favBtn = Instance.new("TextButton", ItemFrame);
						favBtn.AutoLocalize = false;
						favBtn.Size = UDim2.new(0.15, 0, 1, 0);
						FlatIdent_324DE = 6;
					end
					if (7 == FlatIdent_324DE) then
						function UpdateStar()
							local FlatIdent_68E92 = 0;
							local favs;
							local isFav;
							while true do
								if (FlatIdent_68E92 == 1) then
									favBtn.Text = (isFav and "â­") or "â˜†";
									break;
								end
								if (FlatIdent_68E92 == 0) then
									favs = GetFavorites();
									isFav = favs[originGame] and table.find(favs[originGame], name);
									FlatIdent_68E92 = 1;
								end
							end
						end
						UpdateStar();
						favBtn.TextColor3 = Color3.new(1, 1, 0);
						FlatIdent_324DE = 8;
					end
					if (FlatIdent_324DE == 6) then
						favBtn.Position = UDim2.new(0.85, 0, 0, 0);
						favBtn.BackgroundColor3 = Color3.fromRGB(60, 0, 110);
						UpdateStar = nil;
						FlatIdent_324DE = 7;
					end
					if (FlatIdent_324DE == 0) then
						executeFunc = ((type(data) == "function") and data) or data.func;
						originGame = ((type(data) == "table") and data.originalGameName) or gameData.Name;
						ItemFrame = Instance.new("Frame", ScriptList);
						FlatIdent_324DE = 1;
					end
					if (FlatIdent_324DE == 1) then
						ItemFrame.Size = UDim2.new(1, -10, 0, 35);
						ItemFrame.BackgroundTransparency = 1;
						b = Instance.new("TextButton", ItemFrame);
						FlatIdent_324DE = 2;
					end
					if (8 == FlatIdent_324DE) then
						favBtn.TextSize = 18;
						Instance.new("UICorner", favBtn).CornerRadius = UDim.new(0, 8);
						favBtn.MouseButton1Click:Connect(function()
							local added = ToggleFavorite(name, originGame);
							UpdateStar();
							Notify("FAVORITOS", (added and ("Adicionado: " .. name)) or ("Removido: " .. name));
							if (not added and (gameData.Name == "Favoritos")) then
								local FlatIdent_2F37F = 0;
								local remaining;
								while true do
									if (FlatIdent_2F37F == 1) then
										for _, child in pairs(ScriptList:GetChildren()) do
											if child:IsA("Frame") then
												remaining = remaining + 1;
											end
										end
										if (remaining == 0) then
											LoadScripts({}, {Name="Favoritos"});
										end
										break;
									end
									if (FlatIdent_2F37F == 0) then
										ItemFrame:Destroy();
										remaining = 0;
										FlatIdent_2F37F = 1;
									end
								end
							end
						end);
						break;
					end
					if (FlatIdent_324DE == 3) then
						b.Text = name;
						b.TextColor3 = Color3.new(1, 1, 1);
						b.Font = Enum.Font.GothamBold;
						FlatIdent_324DE = 4;
					end
					if (FlatIdent_324DE == 4) then
						b.TextSize = 14;
						Instance.new("UICorner", b).CornerRadius = UDim.new(0, 8);
						b.MouseButton1Click:Connect(function()
							ShowExecuteConfirmation(name, executeFunc);
						end);
						FlatIdent_324DE = 5;
					end
				end
			end
		end
	end
	local function OpenGame(data)
		for _, v in pairs(ScriptList:GetChildren()) do
			if (v:IsA("Frame") or v:IsA("TextLabel") or v:IsA("TextButton") or v:IsA("UIPadding")) then
				v:Destroy();
			end
		end
		for _, v in pairs(TabArea:GetChildren()) do
			if (v:IsA("TextButton") or v:IsA("TextLabel")) then
				v:Destroy();
			end
		end
		local StatusLabel = Instance.new("TextLabel", TabArea);
		StatusLabel.Size = UDim2.new(1, -10, 0, 30);
		StatusLabel.Position = UDim2.new(0, 5, 0, 5);
		StatusLabel.BackgroundTransparency = 1;
		StatusLabel.Text = "VocÃª estÃ¡ em: " .. data.Name;
		StatusLabel.TextColor3 = Color3.fromRGB(220, 220, 220);
		StatusLabel.Font = Enum.Font.GothamMedium;
		StatusLabel.TextSize = 11;
		StatusLabel.TextWrapped = true;
		local function createTab(txt, y, col, tbl)
			local btn = Instance.new("TextButton", TabArea);
			btn.Size = UDim2.new(1, -10, 0, 50);
			btn.Position = UDim2.new(0, 5, 0, y);
			btn.Text = txt;
			btn.BackgroundColor3 = col;
			btn.TextColor3 = Color3.new(1, 1, 1);
			btn.TextSize = 13;
			btn.Font = Enum.Font.GothamBold;
			Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 10);
			btn.MouseButton1Click:Connect(function()
				LoadScripts(tbl, data);
			end);
		end
		createTab("COM KEY ðŸ”‘", 45, Color3.fromRGB(50, 150, 90), data.Key);
		createTab("SEM KEY âŒ", 105, Color3.fromRGB(160, 50, 50), data.NoKey);
	end
	local FavTabBtn = Instance.new("TextButton", Sidebar);
