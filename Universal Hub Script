CheckBtn.MouseButton1Click:Connect(function()
	if (KeyInput.Text == CorrectKey) then
		local FlatIdent_6E214 = 0;
		local successColor;
		while true do
			if (FlatIdent_6E214 == 2) then
				IniciarHubOriginal();
				break;
			end
			if (FlatIdent_6E214 == 1) then
				task.wait(1.5);
				KeyGui:Destroy();
				FlatIdent_6E214 = 2;
			end
			if (FlatIdent_6E214 == 0) then
				successColor = Color3.fromRGB(50, 255, 50);
				CustomNotify("KEY VERIFICADA", "Acesso concedido, o script irÃ¡ carregar", false, successColor);
				FlatIdent_6E214 = 1;
			end
		end
	else
		CustomNotify("ERRO", "Key incorreta! Tente novamente.", true);
		KeyInput.Text = "";
	end
end)
